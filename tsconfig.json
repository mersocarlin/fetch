{
  "exclude": ["src/fixtures", "src/**/*.spec.ts"],
  "include": ["src"],
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["dom", "esnext"],
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
